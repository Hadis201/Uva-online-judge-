#include<stdio.h>

int main() {
    int x,y,t,i;
    scanf("%d",&t);
    for(i=1;i<=t;i++){
        scanf("%d%d",&x,&y);
        if((x+y)%2==1 || (x-(x+y)/2 < 0)||x<y){
            printf("impossible\n");
   
        }
        else {printf("%d %d\n",(x+y)/2,(x-y)/2);
            
        }
    }
    return 0;
}
